{
  "name": "mikro_bug_report",
  "scripts": {
    "start": "ts-node src/app.ts",
    "migration:create": "mikro-orm migration:create",
    "migration:up": "mikro-orm migration:up",
    "migration:down": "mikro-orm migration:down",
    "migration:blank": "mikro-orm migration:create --blank",
    "seeder:create": "mikro-orm seeder:create",
    "seeder:run": "mikro-orm seeder:run"
  },
  "dependencies": {
    "@mikro-orm/cli": "^6.1.7",
    "@mikro-orm/core": "^6.1.7",
    "@mikro-orm/migrations": "^6.1.7",
    "@mikro-orm/postgresql": "^6.1.7",
    "@mikro-orm/seeder": "^6.1.7",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./mikro-orm.config.ts"
    ]
  }
}
